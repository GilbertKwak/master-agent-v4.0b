# Master Agent System V4.0-B Environment Variables

# API Keys
ANTHROPIC_API_KEY=sk-ant-your-key-here
OPENAI_API_KEY=sk-your-key-here  # Optional

# System Configuration
MASTER_AGENT_VERSION=4.0-B
ENABLE_EXTERNAL_MEMORY=true
ENABLE_EPHEMERAL_WORKERS=true
ENABLE_3TIER_QUALITY=true

# Phase Settings
DEFAULT_PHASE_COUNT=3
WORKER_RETIREMENT_STRATEGY=immediate

# Quality Thresholds
L1_MIN_CITATIONS=2
L2_CONTRADICTION_TOLERANCE=0
L3_IAM_SDAI_THRESHOLD=0.85

# Performance
MAX_PARALLEL_WORKERS=4
WORKER_TIMEOUT_SECONDS=60
CONTEXT_WINDOW_SIZE=200000

# Logging
LOG_LEVEL=INFO
LOG_PROJECT_MEMORY=true
LOG_QUALITY_CHECKS=true

# Directories
PROJECT_MEMORY_DIR=./data/project_memories
PROMPT_DIR=./prompts
LOG_DIR=./logs